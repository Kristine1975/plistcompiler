// Substitution: inherited variables do not have to be enclosed in double
// underscores anymore since vs 0.6

// Pre 0.6
// -------
// plist
// {
// 	dictionary
// 	{		
// 		key "TestShell1" value string __USER__
// 		key "TestShell2" value string __SHELL__
// 		key "TestShell3" value string __DISPLAY__
// 		key "TestShell4" value string __HOME__
// 		key "TestShell5" value string __TERM__
// 		key "TestShell6" value string __PATH__
// 		
// 	}
// }


// 0.6 or greater
// --------------
plist
{
	dictionary
	{		
		key "TestShell1" value string USER
		key "TestShell2" value string SHELL
		key "TestShell3" value string DISPLAY
		key "TestShell4" value string HOME
		key "TestShell5" value string TERM
		key "TestShell6" value string PATH
		
	}
}
